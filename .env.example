# Application Environment
APP_NAME="LLM Resayil Portal"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://llm.resayil.io

# Log Settings
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info

# Database (MySQL)
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=resayili_llm_resayil
DB_USERNAME=resayili_llm
DB_PASSWORD=
DB_PREFIX=

# Redis (for rate limiting)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=

# Ollama API (GPU Server)
OLLAMA_GPU_URL=http://208.110.93.90:11434
OLLAMA_CLOUD_URL=https://cloud.ollama.example.com
CLOUD_API_KEY=

# MyFatoorah Payment Gateway
MYFATOORAH_API_KEY=your_myfatoorah_api_key_here
MYFATOORAH_BASE_URL=https://ap-gateway.myfatoorah.com
MYFATOORAH_CALLBACK_URL=https://llm.resayil.io/webhooks/payment
MYFATOORAH_CURRENCY=KWD

# Resayil WhatsApp API
WHATSAPP_API_URL=https://api.resayil.io/whatsapp
WHATSAPP_API_KEY=your_whatsapp_api_key_here
ADMIN_PHONE=96500000000

# Queue Configuration
QUEUE_CONNECTION=database

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=25
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=hello@example.com
MAIL_FROM_NAME="LLM Resayil Portal"

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Sanctum (API Tokens)
SANCTUM_STATEFUL_DOMAINS=llm.resayil.io

# Breeze (Auth)
BREEZE_ROOT_DOMAIN_URL=https://llm.resayil.io
